(reset)
(watchall)

(deftemplate character (slot name) (slot gender) (slot job) (slot house) (slot wand)  (slot species) (slot blood_status) (slot hair_color) (slot eye_color) (slot loyality) (slot skills))

(defquery search-by-gender
 "Finds people with a gender"
 (declare (variables ?g))
 (character (name ?n) (gender ?g) (job ?j) (house ?h) (wand ?w)  (species ?s) (blood_status ?bs) (hair_color ?hc) (eye_color ?ec) (loyalty ?l) (skills ?sk))
 )